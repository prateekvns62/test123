.catalog-category-view .column.main .products-list .product-item .product-item-info .wrap-item-photo{display:table !important;    width: 100%;    text-align: center;}
.catalog-category-view .column.main .products-list .product-item-details{display:table !important;}
.catalog-category-view .column.main .products-list .product-item-info .price-box .save-price{width: auto !important;}	
.catalog-category-view .column.main .products-list ul.advantages li{text-align:center !important;}
.catalog-category-view .column.main .products-list .product-item-name {    max-width: 100% !important;    text-align: center !important;}	

.catalog-category-view .column.main .products-list .product-item .product-item-info .price-box .old-price {
    width: 50%!important; }
	.catalog-category-view .column.main .products-list .product-item-info .price-box .save-price {width: 50% !important;
    text-align: right;position: absolute; right: 0px;    bottom: 14px;}
	.catalog-category-view .column.main .products-list .product-item-actions{flex-flow:unset !important;}
	.column.main .products-list .product-item .product-item-info .wrap-item-photo{padding: 0 5px 5px 0 !important;}
	.shop-cart-parent form#shipping-zip-form .field[name="shippingAddress.postcode"] .control input {height: 40px;
    max-width: 100%;    width: 100%;}
	.shop-cart-parent .cart-discount .content .field {    width: 100%;display: block;}
	.shop-cart-parent .cart-discount .content .field .control input { height: 40px; width: 100%;}
	.shop-cart-parent .cart-discount .content .actions-toolbar {width: 100%;display: block !important;}
	.shop-cart-parent .cart-discount .content .actions-toolbar button{width:100%;} 
	
}

.checkout-cart-index div[name="shippingAddress.region"] {
      display: none !important;}

.checkout-cart-index div[name="shippingAddress.country_id"]{}
 #checkout-step-shipping #co-shipping-form{display:block !important;}
.checkout-shipping-method .step-title{display:block !important;}

.new_custom_add_to_cart_button+.custom_button{display:none;}
.catalog-category-view .new_custom_add_to_cart_button+.custom_button{display:block;}
.grey-block-info {
    background: #666;
    margin: 0 auto;
    padding: 15px 20px;
    max-width: 1320px;
    box-sizing: border-box;
}